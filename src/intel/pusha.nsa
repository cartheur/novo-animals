NOP
NOP
NOP
LD HL, $FETCH
#80
LD Y, $VMS            # set Y = $VMS on exit
DISHL DZ, PGA         # jump to next VMC
#86

ADDR 0xC0             # else, iden SPH, iden SPH
LD HL, $IDEN$NULL
LD Y, $AREG
FNFH DZ, NA           # A = acc
#21
LDZ Y, $SPH
FNH DZ, Y             # Y = SPH
FNFH A, NM            # A -> [SP]
LD HL, $DEC$NULL
#30
LD Y, $SPL
FNFH DZ, XD           # X,SPL = SPL-1
LD HL, $F2PSW$NULL
LD Y, $FREG
#39
FNDH DZ, NA           # A = PSW
LD HL, $IDEN$NULL
LDZ Y, $SPH
FNH DZ, Y             # Y = SPH
FNFH A, NM            # A -> [SP]
#51
NOP
NOP
LD PG, $PREFETCH_PG
#55
