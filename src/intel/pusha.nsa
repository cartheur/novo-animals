# Push A, PSW (prefetch unless SPH underflow)
INCLUDE ../inc/unary.nsa
INCLUDE ../inc/zpage.nsa
INCLUDE ../inc/pages.nsa
PAGE PUSHA_PG

# assume: Y = $VMS
LDZ HL, $INC2$NULL
FNH DZ, HLD           # inc state
LD HL, $DEC$FORK2
LD Y, $SPL
FNFH DZ, XD           # X,SPL = SPL-1
FNDL A, PC
#14

ADDR 0x40             # SPL = 255, dec SPH, iden SPH
LDZ Y, $SPH
FNH DZ, HLD           # SPH = SPH-1      *** dec SPH ***
LD HL, $IDEN$NULL
#20
LD Y, $AREG
FNFH DZ, NA           # A = acc
LDZ Y, $SPH
FNH DZ, Y             # Y = SPH
#29
FNFH A, NM            # A -> [SP]
LD HL, $DEC$NULL
LD Y, $SPL
FNFH DZ, XD           # X,SPL = SPL-1
#39
LD HL, $F2PSW$NULL
LD Y, $FREG
FNDH DZ, NA           # A = PSW
LD HL, $IDEN$NULL
LDZ Y, $SPH
#50
FNH DZ, Y             # Y = SPH
FNFH A, NM            # A -> [SP]
NOP
NOP
NOP
NOP
NOP
#60
NOP
NOP
NOP
NOP
NOP
NOP
NOP
NOP
NOP
NOP
#70
NOP
NOP
NOP
NOP
NOP
NOP
NOP
NOP
LD HL, $FETCH
#80
LD Y, $VMS            # set Y = $VMS on exit
DISHL DZ, PGA         # jump to next VMC
#86

ADDR 0x80             # SPL = 0, iden SPH, dec SPH
LD HL, $IDEN$NULL
LD Y, $AREG
FNFH DZ, NA           # A = acc
#21
LDZ Y, $SPH
FNH DZ, Y             # Y = SPH
FNFH A, NM            # A -> [SP]
LD HL, $DEC$NULL
#30
LD Y, $SPL
FNFH DZ, XD           # X,SPL = SPL-1
LDZ Y, $SPH
FNH DZ, HLD           # SPH = SPH-1      *** dec SPH ***
#39
LD HL, $F2PSW$NULL
LD Y, $FREG
FNDH DZ, NA           # A = PSW
LD HL, $IDEN$NULL
LDZ Y, $SPH
#50
FNH DZ, Y             # Y = SPH
FNFH A, NM            # A -> [SP]
NOP
NOP
NOP
NOP
NOP
#60
NOP
NOP
